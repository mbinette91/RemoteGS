{
  "manifest_version": 2,

  "name": "Grooveshark Remote",
  "description": "Control your Grooveshark from any mobile device with a browser!",
  "icons": { "128": "images/grooveshark-logo.png" },
  "version": "1.0",
  
  "content_scripts": [
  {
	"matches": ["http://*.grooveshark.com/*"],
	"js": ["js/jQuery.js", "js/extension.js"],
	"run_at": "document_end"
  }
  ]
}
